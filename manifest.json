{
  "manifest_version": 3,
  "name": "whisker",
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "16": "src/icons/icon.png",
      "32": "src/icons/icon.png",
      "48": "src/icons/icon.png",
      "128": "src/icons/icon.png"
    },
    "default_title": "Popup Title",
    "default_popup": "src/popup/index.html"
  },
  "icons": {
    "16": "src/icons/icon.png",
    "32": "src/icons/icon.png",
    "48": "src/icons/icon.png",
    "128": "src/icons/icon.png"
  },
  "description": "this is my whisker extension",
  "content_scripts": [
  {
    "js": [
      "src/content/content.ts"
    ],
    "matches": [
      "http://127.0.0.1:5500/*"
    ],
    "all_frames": true,
    "run_at": "document_end",
    "match_about_blank": true
  }
],
"background": {
  "service_worker": "src/background/service-worker.ts"
}
}