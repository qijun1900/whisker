{
  "manifest_version": 3,
  "name": "whisker",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "description": "this is my whisker extension",
  "content_scripts": [
  {
    "js": [
      "src/content/content.ts"
    ],
    "matches": [
      "http://127.0.0.1:5500/*"
    ],
    "all_frames": true,
    "run_at": "document_end",
    "match_about_blank": true
  }
],
"background": {
  "service_worker": "src/background/service-worker.ts"
}
}