{
  "manifest_version": 3,
  "name": "whisker",
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "16": "public/icons/icon16.png",
      "32": "public/icons/icon32.png",
      "48": "public/icons/icon48.png",
      "128": "public/icons/icon128.png"
    },
    "default_title": "Popup Title",
    "default_popup": "src/popup/index.html"
  },
  "icons": {
    "16": "public/icons/icon16.png",
    "32": "public/icons/icon32.png",
    "48": "public/icons/icon48.png",
    "128": "public/icons/icon128.png"
  },
  "description": "this is my whisker extension",
  "content_scripts": [
  {
    "js": [
      "src/content/content.ts"
    ],
    "matches": [
      "<all_urls>"
    ],
    "all_frames": true,
    "run_at": "document_end",
    "match_about_blank": true
  }
],
"background": {
  "service_worker": "src/background/service-worker.ts"
},
"side_panel": {
  "default_path": "src/contentPage/index.html"
},
"permissions": [
  "sidePanel",
  "tabs",
  "storage"
]
}